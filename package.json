{
  "name": "creeper-land-react",
  "version": "0.1.0",
  "description": "creeper-land-react",
  "main": "./src/index.tsx",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config config/webpack/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack/webpack.prod.js",
    "build:analyze": "npm run build -- --analyze",
    "test": "jest --config config/jest.config.js",
    "lint": "eslint --config config/.eslintrc",
    "lint:css": "stylelint --config config/.stylelintrc.json",
    "lint:fix": "npm run lint src -- --fix && npm run lint:css './src/**/*.{ts,tsx,scss,css}' -- --fix",
    "storybook": "start-storybook -p 6006 -c config/.storybook",
    "build-storybook": "build-storybook -c config/.storybook"
  },
  "author": "https://github.com/Allohamora",
  "license": "ISC",
  "dependencies": {
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.7",
    "clsx": "^1.1.1",
    "normalize.css": "^8.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "styled-components": "^5.2.1",
    "styled-normalize": "^8.0.7",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@storybook/addon-actions": "^6.1.14",
    "@storybook/addon-essentials": "^6.1.14",
    "@storybook/addon-links": "^6.1.14",
    "@storybook/react": "^6.1.14",
    "@svgr/webpack": "^5.5.0",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.4.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-beautiful-sort": "^1.0.6",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "html-webpack-plugin": "^5.0.0-beta.5",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "img-optimize-loader": "^1.0.7",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^1.3.4",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "react-refresh": "^0.9.0",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.14",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.15.0",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --relative && npm run test"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint -- --fix",
      "npm run lint:css -- --fix"
    ],
    "*.{scss,css}": "npm run lint:css -- --fix"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
