{
  "plugins": [
    "stylelint-scss"
  ],
  "extends": [
    "stylelint-config-standard",
    "stylelint-config-sass-guidelines"
  ],
  "rules": {
    "color-named": null,
    "max-nesting-depth": [3],
    "scss/dollar-variable-pattern": null,
    "declaration-property-value-disallowed-list": {}
  }
}